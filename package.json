    {
      "path": "dist/react-use-random-hook.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "husky": "^9.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
