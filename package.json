  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexrah/react-use-random-hook.git"
  },
  "homepage": "https://github.com/alexrah/react-use-random-hook#readme",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
